<%- include ('../partials/head'); %>

<header>
    <%- include ('../partials/navbar'); %>
    <h1>Classrooms</h1>

    <!--classrooms go here-->
    <% classrooms.forEach(function(classroom) { %> 
        <div>
            <p><%= classroom.className %></p>
            <a href="/classroom/gameform/<%= classroom._id %>">play</a>
            <a href="/classroom/update/<%= classroom._id %>"><i class="far fa-edit"></i></a>
            <a href="/classroom/delete/<%= classroom._id %>"><i class="far fa-trash-alt"></i></a>
        </div>
        

    <% }) %>
    
    <a href="/classroom/add">Add Class</a>
    
</header>

